{
  "name": "tigo",
  "description": "All-in-one lightweight cloud service platform",
  "version": "0.6.4",
  "main": "server.js",
  "author": "BackRunner",
  "license": "MIT",
  "files": [
    "server.js",
    "ecosystem.config.js",
    "nodemon.json",
    "LICENSE",
    "scripts",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tigojs/tigo.git"
  },
  "homepage": "https://github.com/tigojs/tigo",
  "scripts": {
    "start": "cross-env NODE_ENV=prod DB_ENV=prod node ./server.js",
    "dev": "cross-env NODE_ENV=dev DB_ENV=prod nodemon ./server.js",
    "dev:db": "cross-env NODE_ENV=dev DB_ENV=dev nodemon ./server.js",
    "upgrade:utils": "node ./scripts/upgradeUtils.js",
    "link:packages": "node ./scripts/linkPackages.js",
    "link:global": "node ./scripts/linkToGlobal.js",
    "publish:packages": "node ./scripts/publishPackages.js",
    "publish:lambda-packages": "cross-env PACKAGES_DIR_PATH=\"./lambda-packages\" node ./scripts/publishPackages.js",
    "publish:packages:force": "cross-env IGNORE_NOTFOUND=true node ./scripts/publishPackages.js",
    "publish:lambda-packages:force": "cross-env INGORE_NOTFOUND=true PACKAGES_DIR_PATH=\"./lambda-packages\" node ./scripts/publishPackages.js",
    "migrate": "node ./scripts/migrate.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@tigojs/api-proxy": "file:packages/api-proxy",
    "@tigojs/auth": "file:packages/auth",
    "@tigojs/cfs": "file:packages/cfs",
    "@tigojs/core": "^0.1.0",
    "@tigojs/faas": "file:packages/faas",
    "@tigojs/faas-log": "file:packages/faas-log",
    "@tigojs/faas-performance": "file:packages/faas-performance",
    "@tigojs/fepanel": "file:packages/fepanel",
    "@tigojs/hostbinder": "file:packages/hostbinder",
    "@tigojs/leveldb": "file:packages/leveldb",
    "@tigojs/mongodb": "file:packages/mongodb",
    "@tigojs/mysql": "file:packages/mysql",
    "@tigojs/oss": "file:packages/oss",
    "@tigojs/oss-local": "file:packages/oss-local",
    "@tigojs/sqlite": "file:packages/sqlite",
    "@tigojs/utils": "^0.6.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compress": "^5.1.0",
    "koa-logger": "^3.2.1",
    "koa-parameter": "^3.0.1",
    "koa-tree-router": "^0.8.0",
    "log4js": "^6.3.0",
    "mime": "^2.5.2"
  },
  "devDependencies": {
    "compare-versions": "^3.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "npm-api": "^1.0.1"
  }
}
